{
	"name": "DelimitedText2",
	"properties": {
		"linkedServiceName": {
			"referenceName": "Copydatafromsourcegit",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Filecopyoutput": {
				"type": "Array",
				"defaultValue": [
					"main/Coaches.csv",
					"main/athletes.csv",
					"main/medals.csv",
					"main/teams.csv"
				]
			},
			"filescopyoutput": {
				"type": "String",
				"defaultValue": "\"main/Coaches.csv.csv\",\"athletes.csv\",\"medals.csv\",\"teams.csv\""
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": {
					"value": "@dataset().filescopyoutput",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"rowDelimiter": "\n",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": null
		},
		"schema": []
	}
}