{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Olympicsdf1997"
		},
		"Copydatafromsourcegit1997_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Copydatafromsourcegit1997'"
		},
		"CopytoAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'CopytoAzureDataLakeStorage'"
		},
		"Copydatafromsourcegit1997_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/praddyjoshi/"
		},
		"Copydatafromsourcegit1997_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "praddyjoshi"
		},
		"CopytoAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://olympicsrg1997.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Data Ingestion from Git to ADLS')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "ForEach1",
						"type": "ForEach",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@pipeline().parameters.DataIngestionfromJson",
								"type": "Expression"
							},
							"activities": [
								{
									"name": "CopyDatafromGit",
									"type": "Copy",
									"dependsOn": [],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "DelimitedTextSource",
											"storeSettings": {
												"type": "HttpReadSettings",
												"requestMethod": "GET"
											},
											"formatSettings": {
												"type": "DelimitedTextReadSettings"
											}
										},
										"sink": {
											"type": "DelimitedTextSink",
											"storeSettings": {
												"type": "AzureBlobFSWriteSettings"
											},
											"formatSettings": {
												"type": "DelimitedTextWriteSettings",
												"quoteAllText": true,
												"fileExtension": ".txt"
											}
										},
										"enableStaging": false,
										"translator": {
											"type": "TabularTranslator",
											"typeConversion": true,
											"typeConversionSettings": {
												"allowDataTruncation": true,
												"treatBooleanAsNumber": false
											}
										}
									},
									"inputs": [
										{
											"referenceName": "CopydatafromGit",
											"type": "DatasetReference",
											"parameters": {
												"Copydatafromgit": {
													"value": "@item().Copydatafromgit",
													"type": "Expression"
												}
											}
										}
									],
									"outputs": [
										{
											"referenceName": "CopydatatoADLSsink",
											"type": "DatasetReference",
											"parameters": {
												"FileNamestoADLS": {
													"value": "@item().FileNamestoADLS",
													"type": "Expression"
												}
											}
										}
									]
								}
							]
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"DataIngestionfromJson": {
						"type": "array",
						"defaultValue": [
							{
								"Copydatafromgit": "Olympics/blob/main/Coaches.csv",
								"FileNamestoADLS": "Coaches.csv"
							},
							{
								"Copydatafromgit": "Olympics/blob/main/athletes.csv",
								"FileNamestoADLS": "athletes.csv"
							},
							{
								"Copydatafromgit": "Olympics/blob/main/medals.csv",
								"FileNamestoADLS": "medals.csv"
							},
							{
								"Copydatafromgit": "Olympics/blob/main/teams.csv",
								"FileNamestoADLS": "teams.csv"
							}
						]
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/CopydatafromGit')]",
				"[concat(variables('factoryId'), '/datasets/CopydatatoADLSsink')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CopydatafromGit')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "Copydatafromsourcegit1997",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"Copydatafromgit": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "HttpServerLocation",
						"relativeUrl": {
							"value": "@dataset().Copydatafromgit",
							"type": "Expression"
						}
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/Copydatafromsourcegit1997')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CopydatatoADLSsink')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "CopytoAzureDataLakeStorage",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"FileNamestoADLS": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@dataset().FileNamestoADLS",
							"type": "Expression"
						},
						"folderPath": "Bronze",
						"fileSystem": "olympics-data"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/CopytoAzureDataLakeStorage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Copydatafromsourcegit1997')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "HttpServer",
				"typeProperties": {
					"url": "[parameters('Copydatafromsourcegit1997_properties_typeProperties_url')]",
					"enableServerCertificateValidation": true,
					"authenticationType": "Basic",
					"userName": "[parameters('Copydatafromsourcegit1997_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Copydatafromsourcegit1997_password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/CopytoAzureDataLakeStorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('CopytoAzureDataLakeStorage_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('CopytoAzureDataLakeStorage_accountKey')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}